{
  "name": "Rando",
  "description": "A random generator for ttrpg gms",
  "version": "1.0.0",
  "author": "brine",
  "license": "MIT",
  "main": "index.html",
  "standard": {
    "globals": [
      "location"
    ]
  },
  "scripts": {
    "pretty": "standard --fix",
    "pack": "webpack",
    "purge": "curl -X POST \"https://api.cloudflare.com/client/v4/zones/$BRINE_TOKEN/purge_cache\" -H \"Authorization: Bearer $BEARER\" -H \"Content-Type: application/json\" --data '{\"purge_everything\":true}'",
    "server": "http-server -p 4242",
    "start": "npm run pack; npm run server"
  },
  "dependencies": {
    "@babel/plugin-transform-object-rest-spread": "^7.22.15",
    "http-server": "^14.1.1",
    "standard": "^17.1.0",
    "webpack": "^5.2.0",
    "webpack-cli": "^5.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-proposal-throw-expressions": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.0",
    "sitemap-webpack-plugin": "^1.1.1"
  }
}
